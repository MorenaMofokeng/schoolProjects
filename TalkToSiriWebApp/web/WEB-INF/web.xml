<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>StartSessionServlet</servlet-name>
        <servlet-class>za.ac.tut.web.StartSessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenderServlet</servlet-name>
        <servlet-class>za.ac.tut.web.GenderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeamSupportedServlet</servlet-name>
        <servlet-class>za.ac.tut.web.TeamSupportedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndServlet</servlet-name>
        <servlet-class>za.ac.tut.web.EndServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartSessionServlet</servlet-name>
        <url-pattern>/StartSessionServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenderServlet</servlet-name>
        <url-pattern>/GenderServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeamSupportedServlet</servlet-name>
        <url-pattern>/TeamSupportedServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndServlet</servlet-name>
        <url-pattern>/EndServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
        <param-name>computer_name</param-name>
        <param-value>Siri</param-value>
    </context-param>
</web-app>
